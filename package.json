{
  "name": "ember-web-animations-next-polyfill",
  "version": "1.0.2",
  "description": "An Ember addon that provides and enables use of the web-animations-next polyfill in consuming applications",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "test:blueprint": "node ./node-tests/blueprints/ember-web-animations-next-polyfill-test.js",
    "test:ci": "npm test && npm run test:blueprint"
  },
  "repository": "https://github.com/BrianSipple/ember-web-animations-next-polyfill",
  "engines": {
    "node": ">= 0.12.0"
  },
  "author": "Brian Sipple <@Brian_Sipple>",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.7.9",
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^2.4.1",
    "ember-cli": "2.11.0",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars": "^1.0.10",
    "ember-cli-htmlbars-inline-precompile": "^0.3.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-jshint": "^2.0.1",
    "ember-cli-qunit": "^3.0.1",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
