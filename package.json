    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.6.3",
    "ember-resolver": "^2.0.3",
    "ember-welcome-page": "^2.0.3",
    "loader.js": "^4.0.10"
  },
  "keywords": [
    "ember-addon",
    "web animation",
    "web animations api",
    "waapi",
    "ember animation",
    "ember polyfill"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.7",
    "fs-extra": "^2.0.0",
    "rsvp": "^3.3.3"
